{
	"name": "svelte5-cloudflare-template",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "pnpm run build && wrangler dev",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"db:push": "drizzle-kit push",
		"db:push:prod": "NODE_ENV=production drizzle-kit push",
		"db:migrate:prod": "NODE_ENV=production drizzle-kit migrate",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio:prod": "NODE_ENV=production drizzle-kit studio",
		"db:studio": "drizzle-kit studio",
		"deploy": "pnpm run build && wrangler deploy",
		"cf-typegen": "wrangler types && mv worker-configuration.d.ts src/",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20251202.0",
		"@inlang/paraglide-js": "^2.5.0",
		"@internationalized/date": "^3.10.0",
		"@lucide/svelte": "^0.544.0",
		"@playwright/test": "^1.56.1",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/postcss": "^4.1.17",
		"@types/better-sqlite3": "^7.6.13",
		"@types/eslint": "^9.6.1",
		"@vitest/browser-playwright": "^4.0.10",
		"autoprefixer": "^10.4.22",
		"better-sqlite3": "^12.5.0",
		"bits-ui": "2.14.4",
		"clsx": "^2.1.1",
		"drizzle-kit": "^0.31.7",
		"embla-carousel-svelte": "^8.6.0",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"formsnap": "2.0.1",
		"globals": "^16.5.0",
		"lucide-svelte": "^0.555.0",
		"mode-watcher": "^1.1.0",
		"playwright": "^1.56.1",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"svelte": "^5.45.5",
		"svelte-check": "^4.3.4",
		"svelte-sonner": "^1.0.7",
		"sveltekit-superforms": "^2.28.1",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.17",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.1",
		"vite": "^7.2.6",
		"vitest": "^4.0.10",
		"vitest-browser-svelte": "^2.0.1",
		"wrangler": "^4.52.1",
		"zod": "^4.1.13"
	},
	"dependencies": {
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@sentry/sveltekit": "^10.29.0",
		"@stripe/stripe-js": "^8.5.3",
		"@tailwindcss/vite": "^4.1.17",
		"@types/nprogress": "^0.2.3",
		"better-auth": "1.4.5",
		"drizzle-orm": "^0.45.0",
		"drizzle-zod": "^0.8.3",
		"embla-carousel-autoplay": "^8.6.0",
		"embla-carousel-fade": "^8.6.0",
		"nanoid": "^5.1.6",
		"nprogress": "^0.2.0",
		"resend": "^6.5.2",
		"stripe": "^20.0.0",
		"svelte-confetti": "^2.3.2",
		"svelte-stripe": "^1.4.0",
		"sveltekit-search-params": "^3.0.0"
	}
}
